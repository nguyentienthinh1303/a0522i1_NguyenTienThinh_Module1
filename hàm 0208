<body>
<script>
  function ArrReverse() {
    let arr=[];
    let length=parseInt(prompt("Nhập độ dài của mảng:"));
    for(let i=0;i < length;i++){
      arr[i]=parseInt(prompt("Nhập các phần tử số của mảng:"));
    }
    document.write("Mảng trước khi đảo ngược :"+arr);
    arr.reverse();
    return arr;
  }
  document.write("<br>Mảng sau khi đảo ngược: "+ArrReverse());
</script>
</body>
