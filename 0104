<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
<script>
    var a = parseInt(prompt("Nhập số lượng số cần hiển thị trong dãy Fibonacci"));
    var n1 = 0;
    var n2 = 1;
    var stt = n1 + n2;
    for (var i = 1; i <= a; i++) {
        document.write(n1 + '<br>');
        n1 = n2;
        n2 = stt;
        stt = n1 + n2;
    }
    for (j = 1; j < stt ; j++) {
        if (j % 5 == 0) {
            document.writeln('Số đầu tiên chia hết cho 5 là ' + j );
            break;
        }
    }
</script>
</body>
</html>
