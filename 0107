<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>0107</title>
</head>
<body>
<script>
    let n = 1;
    for (1 <= n; n <= 100; n++){
        if (n % 15 == 0) {
            document.write("FizzBuzz" + '<br>');
        }
        else if (n % 3 == 0){
            document.write("Fizz" + '<br>');
        }
        else if (n % 5 == 0){
            document.write("Buzz" + '<br>');
        }
        else
            document.write(n + '<br>');
    }
</script>
</body>
</html>
