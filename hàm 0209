<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>hàm 0209</title>
</head>
<body>
<script>
  function Array() {
    let length=parseInt(prompt("Nhập độ dài mảng"));
    let arr=[];
    for(let i=0;i<length;i++){
      arr[i]=prompt("Nhập các phần tử cho mảng:");
    }
    let str=prompt("Nhập phần tử muốn tìm:");
    let dem=0;
    for(let j=0;j<arr.length;j++) {
      if (arr[j]==str) {
        dem++;
      }
    }
    if(dem>0){
      return dem;
    }else {
      return -1;
    }
  }
  alert(Array());
</script>
</body>
</html>
