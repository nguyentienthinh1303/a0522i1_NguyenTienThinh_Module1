<script>
  var arr = [0,"f",15,"j","k","@",'$',7,20,]
  var dem = 0;
  for (i=0;i<arr.length;i++) {
    var a = typeof arr[i];
    if (a === 'number') {
      dem++;
    }
  }
  document.write('Số kí tự số trong mảng: ' + dem);
</script>
