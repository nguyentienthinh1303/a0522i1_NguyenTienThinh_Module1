<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>bảng cửu chương</title>
</head>
<body>
<script language="javascript">
    let sout, i, j;
    sout = "<table border='1' width='1000' cellspacing='1' cellpadding='3'>";
    i = 1,
    j = 2;
    while (i <= 10) {
        sout = sout + "<tr>";
        while (j <= 9) {
            sout = sout + "<td>" +  j + "x" + i + "=" + i * j + "</td>";
            j++;
        }
        sout = sout + "</tr>";
        j = 2;
        i++;
    }
    sout = sout + "</table>";
    document.write(sout);
</script>
</body>
</html>
