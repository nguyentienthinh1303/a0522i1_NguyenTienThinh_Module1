<script>
  function N(a,b,c) {
    let min = a;
    if(min>b){
      min = b;
    }
    if(min>c) {
      min = c;
    }
    return ("số nhỏ nhất là " + min);
  }
  alert(N(parseInt(prompt("Nhập số thứ 1")),
          parseInt(prompt("Nhập số thứ 2")),
          parseInt(prompt("Nhập số thứ 3"))))
</script>
</body>
</html>
