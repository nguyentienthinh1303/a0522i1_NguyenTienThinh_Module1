<script>
  function factorial(a) {
    let b = 1 ;
    for ( let i=1;i<=a;i++){
      b = b * i;
    }
    document.write("Kết quả: " + b);
  }
  let a=parseInt(prompt("Nhập số cần tính giai thừa"));
  factorial(a);
</script>
</body>
