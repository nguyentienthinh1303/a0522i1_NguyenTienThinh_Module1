<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>0201</title>
</head>
<body>
<script>
    var a = parseInt(prompt("Nhập số lượng số cần hiển thị trong dãy Fibonacci"));
    var n1 = 0;
    var n2 = 1;
    var stt = n1 + n2;
    for (var i = 1; i <= a; i++) {
        document.write(n1 + '<br>');
        n1 = n2;
        n2 = stt;
        stt = n1 + n2;
    }
</script>
</body>
</html>
