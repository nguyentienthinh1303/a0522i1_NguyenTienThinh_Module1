<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>if 0104</title>
</head>
<body>
<script>
    let arr = [];
    arr.push(parseInt(prompt("Nhập số a")))
    arr.push(parseInt(prompt("Nhập số b")))
    arr.push(parseInt(prompt("Nhập số c")))

    let max = arr[0];
    for(var i=0;i<arr.length;i++){
        if(arr[i]>arr[0]){
            max = arr[i];
        }
    }
    alert("Phần tử lớn nhất = "+max);
</script>
</body>
</html>
