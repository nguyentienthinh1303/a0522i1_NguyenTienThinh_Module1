<script>
    let a = [
        [1, 2, 1, 24],
        [8, 11, 9, 4],
        [7, 0, 7, 27],
        [7, 4, 28, 14],
        [3, 10, 26, 7]
    ];
    for(let i=0; i<a.length;i++)
    {
        document.write("row "+ i+":  ");
        for(let j in a[i]){
            document.writeln(a[i][j].toString())
        }
        document.write("<br>");
    }
</script>
