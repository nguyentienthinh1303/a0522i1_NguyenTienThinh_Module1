<body>
<script>
    let i;
    let j;
    for (i = 1; i <=5 ;i++) {
        for (j = 1; j <= 9; j++) {
            if (i === 5|| i+4===j||i+j===6) {
                document.write("*");
            } else {
                document.write("&nbsp&nbsp");
            }
        }
        document.write("<br>");
    }
</script>
</body>
