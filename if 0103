<body>
<script>
  let number ;
  number = parseInt(prompt("Nhập số a"));
  if(number<0){
    document.write("a bé hơn 0");
  }
  else if(number==0){
    document.write("a bằng 0");
  }
  else{
    document.write("a lớn hơn 0")
  }
</script>
</body>
