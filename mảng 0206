<script>
  let a=[];
  for(let i=1; i<=3;i++)
    a[i]=prompt("nhap so thu "+i+" :");
  for(let i=0; i<a.length;i++)
  {
    if(a[i]=="-")
    {
      a[i]="_";
    }
  }
  document.write(a.toString());
</script>
