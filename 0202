<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>0202</title>
</head>
<body>
<script>
    let a = parseInt(prompt("Nhập số nguyên dương"));
    while (a < 1) {
        a = parseInt(prompt("Nhập lại số nguyên dương"));
    }
    let b = 1;
    for(let i = 1; i <= a; i++){
        b = b * i;
    }
    document.write(b );
</script>
</body>
</html>
